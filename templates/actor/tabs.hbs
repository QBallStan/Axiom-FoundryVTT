<div class="main">

  <nav class="sheet-tabs" data-group="main">

    <button type="button" class="item" data-tab="skills" title="Skills">
      <i class="fas fa-graduation-cap"></i>
    </button>

    <button type="button" class="item" data-tab="combat" title="Combat">
      <i class="fas fa-fist-raised"></i>
    </button>

    <button type="button" class="item" data-tab="inventory" title="Inventory">
      <i class="fas fa-box-open"></i>
