    <button type="button" class="item" data-tab="details" title="Details">
      <i class="fas fa-info-circle"></i>
    </button>

  </nav>

  <section class="sheet-content">

    <div class="tab" data-tab="skills">
      {{> "systems/axiom/templates/actor/tabs/skills.hbs"}}
    </div>

    <div class="tab" data-tab="combat">
      {{> "systems/axiom/templates/actor/tabs/combat.hbs"}}
    </div>

    <div class="tab" data-tab="inventory">
      {{> "systems/axiom/templates/actor/tabs/inventory.hbs"}}
    </div>

    <div class="tab" data-tab="details">
      {{> "systems/axiom/templates/actor/tabs/details.hbs"}}
    </div>

  </section>

</div>

